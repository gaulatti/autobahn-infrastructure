{
  "name": "autobahn",
  "version": "0.1.0",
  "bin": {
    "autobahn": "bin/autobahn.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "cdk": "cdk",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "deploy": "npm run build && cdk deploy --require-approval never",
    "build:synth": "npm run build && cdk synth"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "^2.174.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.721.0",
    "@aws-sdk/client-ecs": "^3.722.0",
    "@aws-sdk/client-lambda": "^3.721.0",
    "@aws-sdk/client-s3": "^3.722.0",
    "@aws-sdk/client-secrets-manager": "^3.721.0",
    "@aws-sdk/client-sns": "^3.721.0",
    "@aws-sdk/util-dynamodb": "^3.721.0",
    "aws-cdk-lib": "^2.174.0",
    "axios": "^1.7.9",
    "constructs": "^10.4.2",
    "dotenv": "^16.4.7"
  }
}
